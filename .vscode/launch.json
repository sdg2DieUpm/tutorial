  "inputs": [
		{
			"id": "cortexTargetID",
      "type": "command",
      "command": "extension.commandvariable.pickStringRemember",
      "args": {
        "key": "lasttarget",
        "description": "Enter the target:",
        "default": "main",
        "options": [
          "main",
          "test_blink",
          "test_led",
          "test_stm32f4"
        ]
      }
		},
    {
			"id": "nativeTargetID",
      "type": "command",
      "command": "extension.commandvariable.pickStringRemember",
      "args": {
        "key": "lasttarget",
        "description": "Enter the target:",
        "default": "main",
        "options": [
          "main",
          "test_blink",
          "test_led",
        ]
      }
		},
	]
